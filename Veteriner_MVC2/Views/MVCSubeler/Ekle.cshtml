@model Veteriner_MVC2.Models.MVCSubelerModel

@{
    ViewBag.Title = "Ekle";
}

<h2>Ekle</h2>


<div class="form-body">
    @using (Html.BeginForm())
    {
        @Html.HiddenFor(Model => Model.SubeNo)

        <div class="form-control">
            @Html.LabelFor(model => model.SubeAdi)<br /><br />

        </div>
        <div class="form-control">
            @Html.EditorFor(Model => Model.SubeAdi)
            @Html.ValidationMessageFor(model => model.SubeAdi)


        </div>

        <div class="form-control">
            @Html.LabelFor(model => model.CalisanSayisi)<br /><br />
        </div>
        <div class="form-control">
            @Html.EditorFor(Model => Model.CalisanSayisi)
            @Html.ValidationMessageFor(model => model.CalisanSayisi)


        </div>

        <div class="form-control">
            @Html.LabelFor(model => model.HizmetSayisi)<br /><br />
        </div>

        <div class="form-control">
            @Html.EditorFor(Model => Model.HizmetSayisi)
            @Html.ValidationMessageFor(model => model.HizmetSayisi)


        </div>

     

        <div class="form-control">

            <input type="submit" value="Kaydet" class="btn btn-success" />
            <input type="reset" value="Reset" class="btn btn-success" />


        </div>

    }

</div>

@section scripts{
    @Scripts.Render("~/bundles/jqueryval");

}


